			</menu>
		</menupopup>
		<menupopup id="coomanPlus_inforow_drag_menu">
			<menuseparator coomanPlus="true"/>
			<menuitem coomanPlus="true" label="&infoRow.move.up;" value="up" oncommand="return coomanPlus.infoRowContextExec(event)"/>
			<menuitem coomanPlus="true" label="&infoRow.move.down;" value="down" oncommand="return coomanPlus.infoRowContextExec(event)"/>
		</menupopup>
		<tooltip id="expiresProgressTooltip" onpopupshowing="coomanPlus.expiresProgress.init(true)" onpopuphiding="if (!coomanPlus.prefExpireProgress &amp;&amp; !coomanPlus.prefExpireCountdown)coomanPlus.expiresProgress.cancel(true)"/>
	</popupset>
	<toolbox id="menu">
		<menubar>
			<menu label="&menu.file;" type="menu" accesskey="&menu.file.accesskey;">
				<menupopup accessibleType="100F">
					<menuitem id="menu_add"
										label="&WindowedCookieEditor.toolbox_box.add.label;"
										oncommand="coomanPlus.openAdd()"
