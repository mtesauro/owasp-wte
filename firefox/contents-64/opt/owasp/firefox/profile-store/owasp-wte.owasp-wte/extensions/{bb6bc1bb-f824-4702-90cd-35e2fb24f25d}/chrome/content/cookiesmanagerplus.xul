	</toolbox>
	<vbox id="main" flex="1" class="contentPane">
			<hbox align="center" id="nav-bar">
				<label control="lookupcriterium">&filter.label;</label>
				<textbox	id="lookupcriterium"
									default="true"
									flex="1"
									onkeyup="if (event.keyCode == 13) coomanPlus.doLookup(event);"
									oncommand="if (coomanPlus.prefAutoFilter &amp;&amp; coomanPlus.checkFilter()) coomanPlus.doLookup()"
									persist="filter"
									filter=""
									type="search"
									aria-controls="cookiesTree"
									accessibleType="101B"
				/>
				<button	id="filtermenu"
								type="menu"
								tooltiptext="&WindowedCookieEditor.filter.label;"
								label="&WindowedCookieEditor.filter.label;"
								class="menubutton"
								accessibleType="1002"
				>
					<menupopup id="filter" oncommand="coomanPlus.changeFilter(event);" accessibleType="100F">
						<menuitem label=" "
											description="&WindowedCookieEditor.filter.label;"
											class="menuheader"
											disabled="true"
						/>
						<menuseparator accessibleType="1010"/>
						<menuitem id="searchname"
											type="checkbox"
											closemenu="none"
											label="&WindowedCookieEditor.filter.name;"
											accessibleType="100E"
						/>
						<menuitem id="searchhost"
											type="checkbox"
											closemenu="none"
											label="&WindowedCookieEditor.filter.host;"
											accessibleType="100E"
						/>
						<menuitem id="searchcontent"
											type="checkbox"
											closemenu="none"
											label="&WindowedCookieEditor.filter.content;"
											accessibleType="100E"
						/>
						<menuseparator accessibleType="1010"/>
						<menuitem id="searchcase"
											type="checkbox"
											closemenu="none"
											label="&WindowedCookieEditor.filter.case;"
											accessibleType="100E"
						/>
					</menupopup>
				</button>
				<button id="lookupstart" oncommand="coomanPlus.doLookup()" accessibleType="1002"/>
			</hbox>
			<tree id="cookiesTree"
						flex="1"
						hidecolumnpicker="false"
						treelines="true"
						seltype="multiple"
						persist="scrollPos selectedHost selectedPath selectedName sortDirection sortResource hidden"
						editable="true"
						enableColumnDrag="true"
						scrollPos=""
						seletedHost=""
						selectedPath=""
						selectedName=""
						sortDirection="ascending"
						sortResource="rawHost"
						accessibleType="101D"
						dragable="false"
			>
				<treecols pickertooltiptext="&tree.picker;" accessibleType="101E">
					<treecol	id="sel"
										flex="0"
										onclick="if(!event.button)coomanPlus.selectAllTogle();"
										type="checkbox"
										context="coomanPlus_tree_menu"
										tooltiptext="&WindowedCookieEditor.toolbox_box.select.tooltip;"
										ignoreincolumnpicker="true"
										fixed="true"
										align="center"
										pack="center"
										editable="true"
										persist="ordinal"
					/>
					<splitter style="display: none;"/>
					<treecol	id="rawHost"
										label="&cookiedomain.label;"
										flex="3"
										onclick="if(!event.button)coomanPlus.cookieColumnSort(this.id);"
										context="coomanPlus_tree_menu"
										tooltiptext="&WindowedCookieEditor.toolbox_box.select.tooltip;"
										ignoreincolumnpicker="true"
										persist="width hidden ordinal"
					/>
					<splitter class="tree-splitter"/>
					<treecol	id="name"
										label="&cookiename.label;"
										flex="2"
										onclick="if(!event.button)coomanPlus.cookieColumnSort(this.id);"
										context="coomanPlus_tree_menu"
										tooltiptext="&WindowedCookieEditor.toolbox_box.select.tooltip;"
										ignoreincolumnpicker="true"
										persist="width hidden ordinal"
					/>
					<splitter class="tree-splitter"/>
					<treecol	id="value"
										label="&cookievalue.label;"
										flex="1"
										onclick="if(!event.button)coomanPlus.cookieColumnSort(this.id);"
										context="coomanPlus_tree_menu"
										tooltiptext="&WindowedCookieEditor.toolbox_box.select.tooltip;"
										hidden="true"
										persist="width hidden ordinal"
					/>
					<splitter class="tree-splitter"/>
					<treecol	id="path"
										label="&cookiepath.label;"
										flex="1"
										onclick="if(!event.button)coomanPlus.cookieColumnSort(this.id);"
										context="coomanPlus_tree_menu"
										tooltiptext="&WindowedCookieEditor.toolbox_box.select.tooltip;"
										hidden="true"
										persist="width hidden ordinal"
					/>
					<splitter class="tree-splitter"/>
					<treecol	id="expires"
										label="&cookieexpire.label;"
										flex="1"
										onclick="if(!event.button)coomanPlus.cookieColumnSort(this.id);"
										context="coomanPlus_tree_menu"
										tooltiptext="&WindowedCookieEditor.toolbox_box.select.tooltip;"
										hidden="true"
										persist="width hidden ordinal"
					/>
					<splitter class="tree-splitter"/>
					<treecol	id="creationTimeString"
										display="&cookiecreationTime.label; *"
										label="&cookiecreationTime.label;"
										flex="1"
										onclick="if(!event.button)coomanPlus.cookieColumnSort(this.id);"
										context="coomanPlus_tree_menu"
										tooltiptext="&WindowedCookieEditor.toolbox_box.select.tooltip;"
										hidden="true"
										persist="width hidden ordinal"
					/>
					<splitter class="tree-splitter"/>
					<treecol	id="lastAccessedString"
										display="&cookielastAccessed.label; *"
										label="&cookielastAccessed.label;"
										flex="1"
										onclick="if(!event.button)coomanPlus.cookieColumnSort(this.id);"
										context="coomanPlus_tree_menu"
										tooltiptext="&WindowedCookieEditor.toolbox_box.select.tooltip;"
										hidden="true"
										persist="width hidden ordinal"
