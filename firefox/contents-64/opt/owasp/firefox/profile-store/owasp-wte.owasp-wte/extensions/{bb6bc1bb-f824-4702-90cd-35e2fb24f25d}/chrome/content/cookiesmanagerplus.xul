			<menuitem id="tree_menu_unprotect"  label="&cookieculler.unprotect;" accesskey="&cookieculler.unprotect.accesskey;" oncommand="coomanPlus.cookieCuller.unprotect()" accessibleType="100E"/>
			<menuseparator id="separator_export" accessibleType="1010"/>
			<menuitem id="menu_exportclipboard"  label="&export.clipboard;" accesskey="&export.clipboard.accesskey;" oncommand="coomanPlus.exportClipboard()" accessibleType="100E"/>
			<menuitem id="menu_exportfile"  label="&export.file;" accesskey="&export.file.accesskey;" oncommand="coomanPlus.exportFile()" accessibleType="100E"/>
			<menu id="menu_backup"
						type="menu"
						label="&backup.button;"
						accesskey="&backup.button.accesskey;"
			>
				<menupopup accessibleType="100F">
					<menuitem id="menu_backupselected"
										label="&backup.selected;"
										accesskey="&backup.selected.accesskey;"
										oncommand="coomanPlus.backupSelected()"
										accessibleType="100E"
					/>
					<menuitem id="menu_backupall"
										label="&backup.all;"
										accesskey="&backup.all.accesskey;"
										oncommand="coomanPlus.backupAll()"
										accessibleType="100E"
					/>
					<menuseparator accessibleType="1010"/>
					<menuitem id="menu_restoreselected"
										label="&restore.selected;"
										accesskey="&restore.selected.accesskey;"
										oncommand="coomanPlus.restoreSelected()"
										accessibleType="100E"
					/>
					<menuitem id="menu_restoreAll"
										label="&restore.all;"
										accesskey="&restore.all.accesskey;"
										oncommand="coomanPlus.restoreAll()"
										accessibleType="100E"
					/>
				</menupopup>
			</menu>
			<menuseparator accessibleType="1010"/>
			<menu type="menu" label="&menu.view.tree;" accesskey="&menu.view.tree.accesskey;">
				<menupopup id="treeView" onpopupshowing="coomanPlus.treeView(this)" oncommand="coomanPlus.treeViewSelect(event)" accessibleType="100F">
