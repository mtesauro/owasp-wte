										image="chrome://cookieculler/skin/cookiecullersmall.png"
										label="&cookieculler.open;"
										accesskey="&cookieculler.open.accesskey;"
										oncommand="coomanPlus.cookieCuller.open()"
										class="menuitem-iconic"
					/>
					<menuseparator accessibleType="1010"/>
					<menuitem image="chrome://cookiesmanagerplus/skin/images/options16x16.png"
										oncommand="coomanPlus.openOptions();"
										label="&WindowedCookieEditor.Options.label;"
										accesskey="&WindowedCookieEditor.Options.accesskey;"
										class="menuitem-iconic"
										accessibleType="100E"
